" looks
syntax enable
set cmdheight=1
set number
set noeb vb t_vb=
set background=dark
set laststatus=2
set term=screen-256color
filetype plugin indent on

" encoding
set enc=utf-8
set fenc=utf-8
set termencoding=utf-8

" 4 spaces master race
set tabstop=4
set expandtab
set shiftwidth=4

" backspace
set backspace=2
